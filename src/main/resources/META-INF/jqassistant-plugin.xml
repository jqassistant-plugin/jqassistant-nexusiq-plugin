<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://jqassistant.github.io/jqassistant/current/schema/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant NexusIQ Plugin" id="jqassistant.plugin.nexusiq"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ActionDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ComponentCoordinateDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ComponentFactDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ComponentIdentifierDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ConditionFactDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ConstraintFactDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.PolicyAlertDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.PolicyEvaluationResultDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ScanReportDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.ScanReportFileDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.TriggerDescriptor</class>
        <class>org.jqassistant.plugin.nexusiq.api.model.scanreport.TriggerReferenceDescriptor</class>
    </model>
    <scope>
        <class>org.jqassistant.plugin.nexusiq.api.NexusIQScope</class>
    </scope>
    <scanner>
        <class>org.jqassistant.plugin.nexusiq.impl.scanreport.ScanReportScannerPlugin</class>
    </scanner>
    <rules>
        <resource>nexusiq.xml</resource>
    </rules>
</jqassistant-plugin>
